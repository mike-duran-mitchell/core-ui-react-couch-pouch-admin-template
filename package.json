{
  "name": "core-ui-react-couch-pouch-admin-template",
  "version": "1.0.0",
  "description":
    "CoreUI (Create-React-App Admin Theme) with CouchDB + Superlogin and PouchDB on the frontend",
  "main": "index.js",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "nodemon",
    "dev":
